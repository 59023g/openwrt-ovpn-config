
config wifi-device 'radio0'
	option type 'mac80211'
	option channel '11'
	option hwmode '11g'
	option path 'platform/qca955x_wmac'
	option htmode 'HT20'

config wifi-iface
	option device 'radio0'
	option mode 'ap'
	option ssid 'ef893989844e'
	option encryption 'psk2'
	option key '>]7Woa,%l1a4/pV$"c=AHi$V*Uu$$@3@{Hul*#pu2R*?D^AEC]ejGhAP>8R@Vc_'
	option network 'lan'

config wifi-device 'radio1'
	option type 'mac80211'
	option channel '36'
	option hwmode '11a'
	option path 'pci0000:01/0000:01:00.0'
	option htmode 'VHT80'
	option txpower '17'
	option country 'US'

config wifi-iface
	option device 'radio1'
	option network 'lan'
	option mode 'ap'
	option ssid 'OpenWrt'
	option encryption 'psk2'
	option key 'monkeybrains'
	option disabled '1'

